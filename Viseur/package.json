  "devDependencies": {
    "@types/git-revision-webpack-plugin": "3.0.1",
    "@types/webpack": "4.41.26",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.18.0",
    "eslint-config-jacobfischer": "github:JacobFischer/eslint-config.git#d2068a3680e5932b61f197a9c0d5d00741dd4130",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.3.3",
    "eslint-plugin-prettier": "3.3.1",
    "git-revision-webpack-plugin": "3.0.6",
    "http-server": "0.12.3",
    "husky": "4.3.8",
    "npm-check-updates": "11.1.1",
    "prettier": "2.2.1",
    "shx": "0.3.3",
    "tslib": "2.1.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:raw": "webpack --progress --colors",
    "build:dev": "npm run build:raw -- --mode development",
    "build:prod": "npm run build:raw -- --mode production",
    "build:stats": "webpack --profile --json > webpack-stats.json",
    "clean": "npm run clean:build && npm run clean:docs && npm run clean:stats",
    "clean:docs": "shx rm -rf docs/",
    "clean:build": "shx rm -rf dist/",
    "clean:stats": "shx rm -rf webpack-stats.json",
    "analyze:stats": "webpack-bundle-analyzer webpack-stats.json",
    "analyze": "npm run build:stats && npm run analyze:stats",
    "docs": "typedoc --tsconfig ./tsconfig.json --out docs --module commonjs",
    "ncu": "ncu -u",
    "lint": "eslint ./ --max-warnings=0",
    "prettier:base": "prettier **/*.{js,ts}",
    "prettier": "npm run prettier:base -- --write",
    "prettier:test": "npm run prettier:base -- --check",
    "js": "http-server dist/",
    "watch": "webpack-dev-server --progress --colors --host 0.0.0.0 --port 8080 --content-base dist/ --mode development",
    "start": "npm run watch",
    "test": "npm run lint && npm run build"
  }
}
